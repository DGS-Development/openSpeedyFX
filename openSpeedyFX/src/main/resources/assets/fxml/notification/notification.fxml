<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="409.0" spacing="10.0" styleClass="background-white" stylesheets="@notification.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fx:id="imgImage" fitHeight="91.0" fitWidth="91.0" pickOnBounds="true" preserveRatio="true">
         <HBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </HBox.margin>
      </ImageView>
      <BorderPane>
         <center>
            <Label fx:id="lblText" text="Label">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font></Label>
         </center>
         <HBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </HBox.margin>
      </BorderPane>
      <BorderPane>
         <center>
            <Button fx:id="btnConfirm" contentDisplay="CENTER" mnemonicParsing="false">
               <graphic>
                  <Label fx:id="lblButtonText" contentDisplay="CENTER" text="Label" textFill="WHITE">
                     <graphic>
                        <ImageView fitHeight="66.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@button.png" />
                           </image>
                        </ImageView>
                     </graphic>
                     <font>
                        <Font name="System Bold" size="15.0" />
                     </font>
                  </Label>
               </graphic>
            </Button>
         </center>
      </BorderPane>
   </children>
</HBox>
